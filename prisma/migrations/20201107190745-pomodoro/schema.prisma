// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id      Int      @default(autoincrement()) @id
  email   String   @unique
  name    String
  status  PomodoroStatus @default(STOPPED)
}


enum PomodoroStatus {
  STARTED
  PAUSE
  STOPPED
}
